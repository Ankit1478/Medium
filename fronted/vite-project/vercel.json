{
    "version": 2,
    "builds": [
        {
            "src": "src/**/*.{js,ts,jsx,tsx}",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "dist"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
}